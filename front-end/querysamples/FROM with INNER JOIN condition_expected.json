{
   "Options" : {
     "SELECT_DISTINCT" : 1
   },
   "Tables" : {
      "Contract" : {
         "co" : {
            "OUTPUT" : {
               "contractID" : {
                  "-1" : "ID"
               }
            },
            "CONDITION" : {
               "companyCode" : {
                  "JOIN" : {
                     "comp.companyCode" : "JOIN_TYPE_STRAIGHT"
                  }
               }
            }
         }
      },
      "Company" : {
         "comp" : {
            "OUTPUT" : {
               "companyName" : {
                  "-1" : "companyName"
               }
            }
         }
      }
   }
}